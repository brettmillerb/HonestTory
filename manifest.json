{
"manifest_version": 2,
    "name": "HonestTory",
    "description": "Replaces the word Tory with a more honest description",
    "version": "0.4",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}